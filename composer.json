{
    "name": "fubhy/graphql-php",
    "description": "A GraphQL parser for PHP.",
    "keywords": [
        "graphql",
        "parser",
        "relay",
        "facebook"
    ],
    "homepage": "https://github.com/fubhy/graphql-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Siemssen",
            "email": "fubhy@fubhy.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Fubhy\\GraphQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fubhy\\GraphQL\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
